{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "data-factory-for-ingestion"
		},
		"linked_service_for_blob_storage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linked_service_for_blob_storage'"
		},
		"linked_service_for_sql_database_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'linked_service_for_sql_database'"
		},
		"linked_service_for_Q2_REST_API_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://jsonplaceholder.typicode.com/users"
		},
		"linked_service_for_sql_database_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "server-for-adf.database.windows.net"
		},
		"linked_service_for_sql_database_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db-for-adf"
		},
		"linked_service_for_sql_database_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "ramanathanlakshmanan"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "data-factory-for-ingestion_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"linked_service_for_blob_storage_connectionString": {
						"value": "[parameters('linked_service_for_blob_storage_connectionString')]"
					},
					"linked_service_for_sql_database_password": {
						"value": "[parameters('linked_service_for_sql_database_password')]"
					},
					"linked_service_for_Q2_REST_API_properties_typeProperties_url": {
						"value": "[parameters('linked_service_for_Q2_REST_API_properties_typeProperties_url')]"
					},
					"linked_service_for_sql_database_properties_typeProperties_server": {
						"value": "[parameters('linked_service_for_sql_database_properties_typeProperties_server')]"
					},
					"linked_service_for_sql_database_properties_typeProperties_database": {
						"value": "[parameters('linked_service_for_sql_database_properties_typeProperties_database')]"
					},
					"linked_service_for_sql_database_properties_typeProperties_userName": {
						"value": "[parameters('linked_service_for_sql_database_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "data-factory-for-ingestion_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'data-factory-for-ingestion_ArmTemplate_0')]"
			]
		}
	]
}